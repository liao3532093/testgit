<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vsofo.automation.mapper.ScriptMapper">
    <resultMap id="scriptRes" type="ScriptItem">
        <result property="id" column="ID"/>
        <result property="title" column="Title"/>
        <result property="detail" column="Detail"/>
        <result property="isEnable" column="IsEnable"/>
        <result property="userId" column="AddUserID"/>
        <result property="addTime" column="AddTime"/>
    </resultMap>
    <!--分页查询脚本方法-->
    <select id="selectScriptMethodAll" parameterType="String" resultMap="scriptRes">
        CALL pro_classname(#{value},1);
    </select>
    <!--分页查询脚本方法个数-->
    <select id="selectScriptMethodCount" parameterType="String" resultType="long">
        CALL pro_classname(#{value},2);
    </select>
    <!--保存脚本方法-->
    <select id="saveScriptMethod" parameterType="String" resultType="long">
        CALL pro_classname(#{value},3);
    </select>
    <!--按ID查询脚本方法-->
    <select id="selectScriptMethodById" parameterType="String" resultMap="scriptRes">
        CALL pro_classname(#{value},4);
    </select>
    <!--查询所有脚本方法和ID-->
    <select id="selectScriptMethodByName" parameterType="String" resultMap="scriptRes">
        CALL pro_classname(#{value},5);
    </select>
    <!--启用禁用-->
    <select id="openCloseScriptMethodById" parameterType="String" resultType="long">
        CALL pro_classname(#{value},6);
    </select>
</mapper>