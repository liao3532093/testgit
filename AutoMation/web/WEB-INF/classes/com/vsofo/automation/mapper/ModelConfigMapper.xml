<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vsofo.automation.mapper.ModelConfigMapper">
    <resultMap id="modelConfigRes" type="ModelConfig">
        <result property="id" column="ID"/>
        <result property="caseModelId" column="CaseModelID"/>
        <result property="paramId" column="ParamID"/>
        <result property="value" column="Param_Value"/>
        <result property="userId" column="AddUserID"/>
        <result property="addTime" column="Addtime"/>
        <result property="valueType" column="ValueType"/>
    </resultMap>
    <!--保存用例模块参数-->
    <select id="saveModelConfigInfo" parameterType="String" resultType="long">
        CALL pro_case_modelparam(#{value},1)
    </select>
    <!--保存时删除-->
    <select id="removeModelConfigById" parameterType="String" resultType="long">
        CALL pro_case_modelparam(#{value},2)
    </select>
    <!--按用例模块ID查询-->
    <select id="selectModelConfigByMid" parameterType="String" resultMap="modelConfigRes">
        CALL pro_case_modelparam(#{value},3)
    </select>
</mapper>