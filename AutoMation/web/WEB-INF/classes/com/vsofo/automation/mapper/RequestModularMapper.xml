<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vsofo.automation.mapper.RequestModularMapper">
    <resultMap id="requestModularRes" type="RequestModular">
        <result property="id" column="ID"/>
        <result property="title" column="Titile"/>
        <result property="value" column="Param_Value"/>
        <result property="modelID" column="Model_ID"/>
        <result property="isEnable" column="IsEnable"/>
        <result property="userId" column="AddUserID"/>
        <result property="addTime" column="AddTime"/>
        <result property="valueType" column="ValueType"/>
    </resultMap>
    <!--分页查询所有模块参数-->
    <select id="selectModlarAll" parameterType="String" resultMap="requestModularRes">
        CALL pro_dict_modelparam(#{value},1)
    </select>
    <!--分页查询所有模块参数个数-->
    <select id="selectModlarCount" parameterType="String" resultType="long">
        CALL pro_dict_modelparam(#{value},2)
    </select>
    <!--保存模块参数-->
    <select id="saveModlarInfo" parameterType="String" resultType="long">
        CALL pro_dict_modelparam(#{value},3)
    </select>
    <!--启用禁用模块参数-->
    <select id="startCloseModlar" parameterType="String" resultType="long">
        CALL pro_dict_modelparam(#{value},4)
    </select>
    <!--通过ID查询模块参数-->
    <select id="selectModlarById" parameterType="String" resultMap="requestModularRes">
        CALL pro_dict_modelparam(#{value},5)
    </select>
    <!--取得所有所有模块参数的ID-->
    <select id="selectModlarByIds" parameterType="String" resultMap="requestModularRes">
        CALL pro_dict_modelparam(#{value},6)
    </select>
    <!--通过请求模块配置ID查询-->
    <select id="selectModlarByMid" parameterType="String" resultMap="requestModularRes">
        CALL pro_dict_modelparam(#{value},7)
    </select>
    <!--统一配置模块参数-->
    <select id="unifiedUpdateModlarInfo" parameterType="String" resultType="long">
        CALL pro_dict_modelparam(#{value},8)
    </select>
</mapper>